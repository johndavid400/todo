{
  "name": "Todo-App",
  "version": "0.0.1",
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "build": "npx tsc",
    "start": "npm run build && node build/index.js",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "postinstalll": "npm run prisma:pull && npm run prisma:generate",
    "test": "jest",
    "test-junit": "jest --ci --reporters=default --reporters=jest-junit",
    "test:unit": "jest --testPathIgnorePatterns ./routes/",
    "test:unit:watch": "jest --watch --testPathIgnorePatterns ./routes/",
    "test:integration": "jest --config ./jest.integration.config.ts --testPathPattern ./routes/ --forceExit "
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-when": "^3.6.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "testcontainers": "^10.7.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
